{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "mint",
    "name": "Nubo Docs",
    "colors": {
        "primary": "#8085ff",
        "light": "#8085ff",
        "dark": "#8085ff"
    },
    "background": {
        "color": {
            "dark": "#0a0a0a",
            "light": "#0a0a0a"
        }
    },
    "favicon": "/favicon.svg",
    "navigation": {
        "tabs": [
            {
                "tab": "Documentation",
                "groups": [
                    {
                        "group": "Overview",
                        "pages": [
                            "index",
                            "concepts-and-terms",
                            "quickstart"
                        ]
                    },
                    {
                        "group": "Deployment Guides",
                        "pages": [
                            {
                                "group": "JavaScript",
                                "icon": "square-js",
                                "pages": [
                                    "deploy-guides/javascript/react",
                                    "deploy-guides/javascript/discordjs"
                                ]
                            },
                            "deploy-guides/static",
                            "deploy-guides/golang",
                            "deploy-guides/rust",
                            "deploy-guides/php",
                            "deploy-guides/static"
                        ]
                    }
                ]
            }
        ],
        "global": {
            "anchors": [
                {
                    "anchor": "Documentation",
                    "href": "https://docs.withnubo.com",
                    "icon": "book-open-cover"
                },
                {
                    "anchor": "Blog",
                    "href": "https://www.withnubo.com/blog",
                    "icon": "newspaper"
                },
                {
                    "anchor": "Release Notes",
                    "href": "https://www.withnubo.com/release-notes",
                    "icon": "memo"
                }
            ]
        }
    },
    "logo": {
        "light": "/logo/logo.png",
        "dark": "/logo/logo.png"
    },
    "navbar": {
        "links": [
            {
                "label": "Support",
                "href": "mailto:support@withnubo.com"
            }
        ],
        "primary": {
            "type": "button",
            "label": "Sign in",
            "href": "https://apps.withnubo.com/login"
        }
    },
    "contextual": {
        "options": [
            "copy",
            "view",
            "chatgpt",
            "claude",
            "perplexity",
            "mcp",
            "cursor",
            "vscode"
        ]
    },
    "footer": {
        "socials": {
            "x": "https://x.com/mintlify",
            "github": "https://github.com/mintlify",
            "linkedin": "https://linkedin.com/company/mintlify"
        }
    }
}
